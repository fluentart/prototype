{
  "name": "prototype",
  "version": "5.0.33",
  "description": "Typescrypt based IoT framework for general purpose remote monitoring/control.",
  "main": "src/main.js",
  "scripts": {
    "start": "node ./build/main.js",
    "test": "mocha ./build/tests.js",
    "tests": "node ./build/tests.js",
    "build": "tsc --lib es2015 ./src/main.ts ./src/tests.ts --outDir ./build",
    "buildwatch": "tsc --sourcemap --w --lib es2015 ./src/main.ts ./src/tests.ts --outDir ./build",
    "build-clean": "rmdir build /s /q && tsc",
    "debug": "cd build && nodemon --inspect main.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:IoT-nxt/prototype.git"
  },
  "keywords": [
    "iot",
    "iotnxt",
    "rpi",
    "node",
    "ts"
  ],
  "author": "Rouan van der Ende",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/geoip-lite": "^1.1.29",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "discord.js": "^11.4.2",
    "express": "^4.16.3",
    "geoip-lite": "^1.3.2",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mongojs": "^2.6.0",
    "mqtt": "^2.18.8",
    "nodemailer": "^4.6.8",
    "react-modal": "^3.8.1",
    "request": "^2.88.0",
    "socket.io": "^2.1.1",
    "source-map-support": "^0.5.9",
    "typescript": "^3.1.3",
    "vm2": "^3.6.3"
  },
  "devDependencies": {}
}
